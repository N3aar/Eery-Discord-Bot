{
	"name": "eery-project",
	"version": "1.0.0",
	"description": "Discord bot for Uni√£o Sinistra server",
	"main": "dist/index.js",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf dist",
		"start": "node .",
		"start:env": "node --env-file=.env .",
		"build": "pnpm run clean && tsc && tsc-alias",
		"dev": "nodemon --exec \"pnpm run build && pnpm run start:env\" --watch src --ext ts",
		"lint": "biome check src",
		"lint:fix": "biome check --apply src",
		"prepare": "husky || true"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@swc/cli": "^0.3.12",
		"@swc/core": "^1.5.7",
		"@types/node": "^20.12.12",
		"husky": "^9.0.11",
		"nodemon": "^3.1.1",
		"prisma": "^5.14.0",
		"rimraf": "^5.0.7",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@prisma/client": "5.14.0",
		"@sapphire/discord.js-utilities": "^7.2.1",
		"@sapphire/framework": "^5.2.1",
		"@sapphire/pieces": "^4.2.2",
		"dayjs": "^1.11.11",
		"discord.js": "14.x"
	}
}
